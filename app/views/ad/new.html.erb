<%= render  partial: "errorlog" %>
<%= form_for(@ad ,url: "/ad",html: {method: "post"}) do |f| %> <%# 入稿フォーム%>
  <%= f.label :name %>
  <%= f.text_area :text,value: @ad.price %>
  <%= f.number_field :price,value: @ad.price %>
  <%= f.number_field :advertiser_id,value: @ad.advertiser_id %>
  <%= f.file_field :image,value: @ad.image %>
  <%= f.submit("入稿") %>
<% end %>
